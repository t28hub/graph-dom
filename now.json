{
  "name": "graph-dom",
  "version": 2,
  "env": {
    "NODE_ENV": "production",
    "GRAPH_DOM_BROWSER_HEADLESS": "true"
  },
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/src/server.js"
    }
  ]
}
