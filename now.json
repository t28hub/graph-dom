{
  "name": "graph-dom",
  "version": 2,
  "env": {
    "NODE_ENV": "production",
    "GRAPH_DOM_SERVER_PORT": "3000",
    "GRAPH_DOM_BROWSER_HEADLESS": "true"
  },
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "src/**/*.graphql"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/server.ts"
    }
  ]
}
