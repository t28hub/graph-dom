{
  "name": "graph-dom",
  "description": "This is a sample AWS lambda functions using the Puppeteer.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": {
    "name": "Tatsuya Maki",
    "email": "t28oogle@gmail.com",
    "url": "https://github.com/t28hub"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:t28hub/graph-dom.git"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "npx now dev --debug",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "npx eslint 'src/**/*.ts'",
    "lint:prettier": "npx prettier --check '*.{js,json,ts}'",
    "test": "npx jest",
    "clean": "npx rimraf coverage dist yarn-error.log",
    "build": "npx webpack --mode production --progress",
    "watch": "npx webpack --mode development --progress --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run test"
    }
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "axios": "^0.19.0",
    "chrome-aws-lambda": "^1.19.0",
    "express": "^4.17.1",
    "graphql": "^14.5.3",
    "puppeteer-core": "^1.19.0",
    "robots-parser": "^2.1.1",
    "source-map-support": "^0.5.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/puppeteer-core": "^1.9.0",
    "@types/source-map-support": "^0.5.0",
    "@types/webpack": "^4.32.2",
    "@types/webpack-node-externals": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "graphql-tag": "^2.10.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-each": "^24.9.0",
    "now": "^16.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "iltorb": "^2.4.3"
  }
