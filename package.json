{
  "name": "graph-dom",
  "description": "This is a sample AWS lambda functions using the Puppeteer.",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": {
    "name": "Tatsuya Maki",
    "email": "t28oogle@gmail.com",
    "url": "https://github.com/t28hub"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:t28hub/graph-dom.git"
  },
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "npx nodemon --exec ts-node src/server.ts",
    "start:now": "npx now dev",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "npx eslint 'src/**/*.ts'",
    "lint:prettier": "npx prettier --check '*.{js,ts}'",
    "test": "yarn test:unit && yarn test:mutation && yarn test:e2e",
    "test:unit": "npx jest --config jest.config.js",
    "test:e2e": "npx jest --config jest.e2e.config.js --runInBand",
    "test:mutation": "npx stryker run",
    "clean": "npx rimraf dist reports yarn-error.log .stryker",
    "build": "npx tsc && npx copyfiles -u 1 src/**/*.graphql dist/src",
    "deploy": "npx now",
    "apollo:publish": "npx apollo schema:publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test:unit"
    }
  },
  "dependencies": {
    "@graphql-modules/core": "^0.7.13",
    "apollo-datasource": "^0.6.3",
    "apollo-server-cache-redis": "^1.1.4",
    "apollo-server-caching": "^0.5.0",
    "apollo-server-errors": "^2.3.4",
    "apollo-server-express": "^2.9.14",
    "apollo-server-plugin-base": "^0.6.8",
    "axios": "^0.19.0",
    "chrome-aws-lambda": "^2.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-query-complexity": "^0.4.1",
    "graphql-toolkit": "0.7.4",
    "helmet": "^3.21.2",
    "log4js": "^6.1.0",
    "puppeteer-core": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "robots-parser": "^2.1.1",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.4.0",
    "@stryker-mutator/html-reporter": "^2.4.0",
    "@stryker-mutator/jest-runner": "^2.4.0",
    "@stryker-mutator/typescript": "^2.4.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/helmet": "^0.0.45",
    "@types/ioredis": "^4.14.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.20",
    "@types/node-fetch": "^2.5.4",
    "@types/puppeteer-core": "^2.0.0",
    "@types/source-map-support": "^0.5.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "apollo": "^2.21.2",
    "copyfiles": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-tag": "^2.10.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-each": "^24.9.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^2.0.2",
    "now": "^16.7.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.0.0",
    "rimraf": "^3.0.0",
    "stryker-cli": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "optionalDependencies": {
    "iltorb": "^2.4.4"
  }
}
